{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"],
    "no-console": ["off"],
    "max-len": ["warn", { "code": 100 }]
  },
  "globals": {
    "ee": "readonly",
    "Map": "readonly",
    "Export": "readonly",
    "ui": "readonly"
  }
}
